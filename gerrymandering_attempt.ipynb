   "source": [
    "import pandas as pd\n",
    "import geopandas as gpd\n",
    "\n",
    "file_path = 'politiche_2022_liste_camera_comuni.csv'\n",
    "df=pd.read_csv(file_path)\n",
    "\n",
    "gdf = gpd.read_file('comuni_italiani_trend_liste_2022_2024.geojson')[[\"name\",\"com_istat_code_num\",\"geometry\"]]\n",
    "gdf\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 32,
   "id": "0884967d",
   "metadata": {},
   "outputs": [],
   "source": [
    "# eliminiamo la prima colonna di df e nazione e quelle inutili \n",
    "\n",
    "df = df.drop(df.columns[0], axis=1)\n",
    "df = df.drop(\"NAZIONE\", axis=1)\n",
    "df = df.drop(\"geometry\", axis=1)\n",
    "df = df.drop(\"name\", axis=1)\n",
    "df\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 33,
   "id": "4f107610",
   "metadata": {},
   "outputs": [],
   "source": [
    "# join tra i due dataframe\n",
    "\n",
    "gdf = gdf.merge(df, left_on='com_istat_code_num', right_on='com_istat_code_num', how='left')\n",
    "gdf\n"
   ]
  },
